  <sub-flow name="check-all-dependencies-are-alive">
    <http:request method="GET" doc:name="Flights: GET /alive" doc:id="d1c7855f-e36d-4a4f-b4d9-ab1167cbd5af" url="https://tngaa-flights-management-sapi-dev.us-e1.cloudhub.io/alive" responseTimeout="950">
