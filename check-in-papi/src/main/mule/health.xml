    </http:request>
    <http:request method="GET" doc:name="Paypal: GET /alive" doc:id="4bc55e7a-9443-4340-a30a-ae57b8c5853a" url="https://tngaa-paypal-sapi-dev.us-e1.cloudhub.io/alive" responseTimeout="950">
