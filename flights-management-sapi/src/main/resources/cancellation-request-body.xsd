<xs:schema
  attributeFormDefault="unqualified"
  elementFormDefault="qualified"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="CancellationNotification">
    <xs:complexType>
      <xs:sequence>
        <xs:element
          type="xs:string"
          name="PNR" />
        <xs:element
          type="xs:string"
          name="PassengerLastName" />
        <xs:any
          minOccurs="0"
          processContents="lax" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>